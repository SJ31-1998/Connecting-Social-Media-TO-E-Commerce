<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	 version="3.0">
    <servlet>
        <servlet-name>FileUploadDBSer</servlet-name>
        <servlet-class>network.FileUploadDBSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeLineShare</servlet-name>
        <servlet-class>network.TimeLineShare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Recommend</servlet-name>
        <servlet-class>network.Recommend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileUploadDBSer</servlet-name>
        <url-pattern>/FileUploadDBSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeLineShare</servlet-name>
        <url-pattern>/TimeLineShare</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Recommend</servlet-name>
        <url-pattern>/Recommend</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
